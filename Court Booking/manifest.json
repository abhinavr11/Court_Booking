{
    "manifest_version": 2,
    "name": "Court Booking",
    "version": "0.0",
    "content_scripts": [
      {
        "matches": [
            "<all_urls>"
        ],
        "js": ["jquery-3.3.1.min.js","content.js"]
      }
    ],
    
    "background": {
      "scripts": ["jquery-3.3.1.min.js","background.js"],
      "persistent": true
    },
    "permissions": ["https://*/*","http://*/*","tabs"],

    "browser_action": {
      "default_icon": "logo.png",
      "default_popup": "index.html",
      "default_title": "Made with ❤️ at IIT B"
    }
    
  } 